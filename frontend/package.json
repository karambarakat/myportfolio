{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "node build_from_mdx.js && qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "graphql": "graphql-codegen",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.14",
    "@builder.io/qwik-city": "^1.2.14",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/builder-vite": "^7.4.6",
    "@storybook/html": "^7.4.6",
    "@storybook/html-vite": "^7.4.6",
    "@types/eslint": "^8.44.4",
    "@types/luxon": "^3.3.3",
    "@types/node": "^20.8.4",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@unocss/preset-mini": "^0.56.5",
    "@unocss/reset": "^0.56.5",
    "@unocss/transformer-variant-group": "^0.57.1",
    "animated-unocss": "^0.0.6",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "^1.2.14",
    "gray-matter": "^4.0.3",
    "prettier": "^3.0.3",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "sanitize-html": "^2.14.0",
    "storybook": "^7.4.6",
    "storybook-framework-qwik": "^0.2.4",
    "tailwindcss-preflight": "^1.0.1",
    "typescript": "^5.2.2",
    "undici": "^5.26.0",
    "unocss": "^0.56.5",
    "vercel": "^29.1.1",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1",
    "yup": "^1.3.2"
  },
  "dependencies": {
    "@qwikest/icons": "^0.0.13",
    "graphql": "^16.8.1",
    "gsap": "^3.12.2",
    "luxon": "^3.4.3",
    "pocketbase": "^0.25.0",
    "unified": "^11.0.3"
  }
}
