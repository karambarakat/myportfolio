{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "Mono repo for my portfolio",
  "private": true,
  "workspaces": [
    "apps/**",
    "pkgs/**",
    "exps/**"
  ],
  "scripts": {
    "start": "turbo run start",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lock": "npm install --package-lock-only --workspaces",
    "tmp:deploy": "cat do_app_spec_prod.yaml | doctl app update 34d6382e-12b8-4e64-89eb-da8b959ab537 --spec -"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karambarakat/myportfolio"
  },
  "author": {
    "name": "Karam Barakat",
    "email": "k99.barakat@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karambarakat/myportfolio/issues"
  },
  "homepage": "https://github.com/karambarakat/myportfolio#readme",
  "packageManager": "pnpm@7.28.0"
}
